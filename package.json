{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --config video=false'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e '",
    "test:e2e:live": "CYPRESS_BASE_URL=https://winter-feather-5776.fly.dev cypress run --config video=false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vite build && npm run lint && vitest run && npm run test:e2e",
    "analyze": "vite build --mode analyze",
    "ship": "npm run test && git push"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "^1.1.36",
    "@vueuse/core": "^12.8.2",
    "pinia": "^3.0.1",
    "vue": "^3.4.27",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@dagger.io/dagger": "^0.16.2",
    "@eslint/eslintrc": "^3.3.0",
    "@rushstack/eslint-patch": "^1.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "14.1.0",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.10",
    "vite": "^6.2.1",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.8"
  }
}
